RewriteEngine on

#  path /page/id
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_URI} !^.*\.(jpg|css|js|gif|png)$ [NC]
RewriteCond %{REQUEST_URI} ^(.*)/(.*)$ [NC]
RewriteRule ^(.*)/(.*)$ index.php?page=$1&id=$2 [QSA,L]
#  path /page/
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_URI} !^.*\.(jpg|css|js|gif|png)$ [NC]
RewriteCond %{REQUEST_URI} ^(.*)$ [NC]
RewriteRule ^(.*)$ index.php?page=$1 [QSA,L]
